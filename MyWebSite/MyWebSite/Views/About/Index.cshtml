@using MyWebSite.Models.Classes
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="about">
    <div class="about-heading">
        <div class="container">
            <h2 class="wow fadeInDown animated" data-wow-delay=".5s">@ViewBag.Title</h2>
            <p class="wow fadeInUp animated" data-wow-delay=".5s">@ViewBag.Message</p>
        </div>
    </div>
    <div class="about-top">
        <div class="container">
            <div class="about-bottom">
                @foreach (var i in Model)
                {
                    <img class="wow fadeInUp animated" data-wow-delay=".5s" src="@i.PhotoUrl" alt="" />
                    <p class="wow fadeInUp animated" data-wow-delay=".5s">@i.Content</p>
                }
            </div>
        </div>
    </div>

    <!-- team -->
    <div class="team">
        <div class="container">
            <div class="popular-heading team-heading">
